package com.niit.furnisets.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;


@Controller
public class FrontController {

	@RequestMapping("/ , home")
	public String home()
	{		
		return "index";
	}
	
	@RequestMapping("tel")
	public String contact()
	{		
		return "contact";
	}
}
